<div id="container" class="m-5 lg:m-10"></div>
<div id="containerDeviceInfo" class="m-5 lg:m-10"></div>

<script>
  window.addEventListener("DOMContentLoaded", async () => {
    try {
      // Get datas
      const response = await fetch(
        `/devices?state=detail&key=<%= deviceKey %>`
      );
      const datas = await response.json();
      if (datas.statusText === "ERROR" || datas.length < 1) {
        document.getElementById(
          "container"
        ).innerHTML += `<div class="bg-white text-red-500 p-4 mt-10 text-xl">
          Device is not have data !
        </div>`;
      } else {
        let arr = datas.lastData;
        let arrLastData = arr.slice(-1);
        arrLastData.map((lastData) => {
          document.getElementById(
            "container"
          ).innerHTML += ` <div class="bg-white p-4 mt-10 space-y-5">
        <!-- Last Data -->
        <h1 class="text-2xl font-extrabold text-center">${lastData.deviceName}</h1>
        <div>
          <div class="flex justify-between">
            <h1 class="text-2xl font-bold text-center lg:text-start underline">
            Last Data
            </h1>
            <div class="flex space-x-3">
               <img src="https://cdn-icons-png.flaticon.com/512/3917/3917267.png" alt="" class="w-10 h-10">
               <div class="justify-self-end">
                <h1 class="text-2xl font-bold text-center lg:text-start">${lastData.createdAt}</h1>
                <h1 class="text-2xl font-bold text-center lg:text-start">${lastData.updatedAt}</h1>
                </div>
              </div>
            </div>
          <div id="liveData" class="flex flex-wrap justify-evenly gap-3 mt-3">
            <div class="w-52 h-52 bg-blue-500 rounded-full grid place-items-center">
              <span class="text-7xl font-bold text-white">${lastData.topic1_VALUE}Â°C</span>
              <span class="text-xl mb-5 text-white">Suhu Air</span>
            </div>
            <div class="w-52 h-52 bg-blue-500 rounded-full grid place-items-center">
              <span class="text-7xl font-bold text-white">${lastData.topic2_VALUE}%</span>
              <h1 class="text-xl mb-5 text-white">Suhu Lingkungan</h1>
              </div>
            <div
              class="w-96 h-32 bg-blue-500 rounded-full lg:mt-5 grid place-items-center">
              <span class="text-7xl font-bold text-white">${lastData.topic3_VALUE}ppm</span>
              <h1 class="text-xl text-white">Total dissolved solid</h1>
            </div>
          </div>
        </div>

        <!-- Chart -->
        <p class="text-2xl font-bold text-center lg:text-start mt-12 underline">
          Overall data by Chart
        </p>
        <div class="flex flex-wrap justify-evenly mt-3">
          <div id="chartContainer">
            <p class="text-2xl font-bold text-center lg:text-start">
              Suhu Air
            </p>
            <canvas id="chartSuhuAir"></canvas>
          </div>
          <div id="chartContainer">
            <p class="text-2xl font-bold text-center lg:text-start">
              Suhu Lingkungan
            </p>
            <canvas id="chartSuhuLingkungan"></canvas>
          </div>
          <div id="chartContainer">
            <p class="text-2xl font-bold text-center lg:text-start">
              Total Dissolved Solid
            </p>
            <canvas id="chartTds"></canvas>
          </div>
        </div>
      </div>`;
        });
        setTimeout(() => {
          // Canvas container
          document.getElementById(`chartSuhuAir`);
          document.getElementById(`chartSuhuLingkungan`);
          document.getElementById(`chartTds`);

          // Chart Suhu air
          new Chart(`chartSuhuAir`, {
            type: "bar",
            data: datas.dataChartSuhuAir,
          });
          // Chart Suhu Lingkungan
          new Chart(`chartSuhuLingkungan`, {
            type: "bar",
            data: datas.dataChartSuhuLingkungan,
          });

          // Chart TDS
          new Chart(`chartTds`, {
            type: "bar",
            data: datas.dataChartTds,
          });
        }, 1000);
      }

      // Device information
      const responseDeviceInfo = await fetch(
        `/devices?state=info&key=<%= deviceKey %>`
      );
      const datasDeviceInfo = await responseDeviceInfo.json();
      datasDeviceInfo.map((devInfo) => {
        document.getElementById(
          "containerDeviceInfo"
        ).innerHTML += ` <div class="bg-white p-4 mt-10 space-y-2 text-xl">
        <h1 class="text-2xl underline">Device specification</h1>
    <div class="flex justify-between pt-5">
      <div class="flex space-x-2">
        <img
          src="https://cdn-icons-png.flaticon.com/512/1698/1698293.png"
          alt="deviceName"
          class="w-8"
        />
        <h1>Name ${devInfo.deviceName}</h1>
      </div>
      <h1>Key <span class="font-bold">${devInfo.deviceKey}</span></h1>
    </div>
    <div class="flex space-x-2">
      <img
        src="https://media.istockphoto.com/id/1320976645/id/vektor/isp-internet-protocol-address-vector-icon-concept-design-data-center-dan-web-hosting-symbol.jpg?s=612x612&w=0&k=20&c=GJXAALxCjvPN5-yJiuDQd1M4S_zxeX1KBD-_hw1INy0="
        alt="deviceName"
        class="w-8"
      />
      <h1>Ip address ${devInfo.deviceIp}</h1>
    </div>
    <div class="flex space-x-2">
      <img
        src="data:image/png;base64,"
        alt="deviceName"
        class="w-8"
      />
      <h1>Subscribe topic</h1>
    </div>
    <ul class="ml-10">
      <li>TOPIC 1 <span class="text-red-500">${devInfo.topic1}</span></li>
      <li>TOPIC 2 <span class="text-red-500">${devInfo.topic2}</span></li>
      <li>TOPIC 3 <span class="text-red-500">${devInfo.topic3}</span></li>
    </ul>
    <div class="flex space-x-2">
      <img
        src="data:image/png;base64,"
        alt="deviceName"
        class="w-8"
      />
      <h1>Created at ${devInfo.updatedAt}</h1>
    </div>
  </div>`;
      });
    } catch (e) {
      console.log(e);
    }
  });
</script>
